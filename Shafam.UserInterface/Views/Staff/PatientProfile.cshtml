@{
    ViewBag.Title = "Patient Details";
}

@model Shafam.Common.DataModel.Patient

@Html.Partial("_Header", @Model)

<form class="form-horizontal" role="form">
    <div class="form-group">
        <label class="col-sm-2 control-label">Patient ID</label>
        <div class="col-sm-10">
            <p class="form-control-static">@Model.PatientId</p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 control-label">First Name</label>
        <div class="col-sm-10">
            <p class="form-control-static">@Model.FirstName</p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 control-label">Last Name</label>
        <div class="col-sm-10">
            <p class="form-control-static">@Model.LastName</p>
        </div>
    </div>
</form>
